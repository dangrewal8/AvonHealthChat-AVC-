{
  "name": "avon-health-rag-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend UI for Avon Health RAG System",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t avonhealth-frontend .",
    "docker:run": "docker run -p 80:80 avonhealth-frontend",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.2",
    "@tanstack/react-query": "^5.13.4",
    "react-router-dom": "^6.20.1",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "tailwindcss": "^3.3.6",
    "postcss": "^8.4.32",
    "autoprefixer": "^10.4.16",
    "terser": "^5.27.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
