# Environment Variables for Full Stack Docker Compose
# Copy this file to .env and fill in your values
#
# Usage:
#   cp .env.example .env
#   # Edit .env with your values
#   docker-compose up

# ==============================================================================
# Avon Health API Configuration (Required)
# ==============================================================================
AVON_CLIENT_ID=your_client_id_here
AVON_CLIENT_SECRET=your_client_secret_here
AVON_BASE_URL=https://demo-api.avonhealth.com

# ==============================================================================
# OpenAI Configuration (Required)
# ==============================================================================
OPENAI_KEY=your_openai_key_here
EMBEDDING_PROVIDER=openai
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=1536

# ==============================================================================
# Cache Configuration
# ==============================================================================
CACHE_ENABLED=true
CACHE_TTL_SECONDS=300

# ==============================================================================
# Vector Database Configuration
# ==============================================================================
VECTOR_DB_TYPE=chromadb
