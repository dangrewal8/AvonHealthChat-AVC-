# Avon Health API OAuth2 Credentials
AVON_CLIENT_ID=your_client_id_here
AVON_CLIENT_SECRET=your_client_secret_here
AVON_BASE_URL=https://demo-api.avonhealth.com

# AI Provider (Ollama - HIPAA Compliant Local Processing)
EMBEDDING_PROVIDER=ollama
LLM_PROVIDER=ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_EMBEDDING_MODEL=nomic-embed-text
OLLAMA_LLM_MODEL=meditron
OLLAMA_MAX_TOKENS=4096
OLLAMA_TEMPERATURE=0.1

# Vector Database Configuration
VECTOR_DB_TYPE=faiss
FAISS_DIMENSION=768
FAISS_INDEX_PATH=/app/data/faiss

# Server Configuration
PORT=3001
NODE_ENV=production

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_SECONDS=300

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
